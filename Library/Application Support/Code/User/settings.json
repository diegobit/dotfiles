    },
    "catppuccin.accentColor": "peach",
    "code-eol.crlfCharacter": "¤¬",
    "code-eol.newlineCharacter": "¬",
    "code-eol.returnCharacter": "¤",
    "codeium.enableCodeLens": false,
    "codeium.enableCommandSideHintEducation": false,
    "codeium.enableConfig": {
        "*": true,
        "markdown": true,
        "plaintext": true,
        "rust": false
    },
    "codeium.enableExplainProblem": true,
    "codeium.indexingRetentionPeriod": 90,
    "debug.console.fontFamily": "MesloLGS-NF-Regular",
    "debug.console.fontSize": 14,
    "debug.console.lineHeight": 18,
    "debug.internalConsoleOptions": "openOnSessionStart",
    "debug.showInStatusBar": "always",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.codeLens": false,
    "editor.codeLensFontFamily": "SF Pro Text",
    "editor.colorDecorators": true,
    "editor.cursorStyle": "line-thin",
    "editor.emptySelectionClipboard": false,
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontLigatures": false,
    "editor.fontSize": 15,
